      "source": [
        "import requests\n",
        "import time\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from datetime import datetime\n",
        "from collections import defaultdict\n",
        "defaultdict\n",
        "import pytz\n",
        "\n",
        "# OpenWeatherMap API details\n",
        "API_KEY = 'c3b4d315ffe7855ff106df4cb928607f' # Replace with your actual API key\n",
        "CITIES = ['Delhi', 'Mumbai', 'Chennai', 'Bangalore', 'Kolkata', 'Hyderabad','Andhra Pradesh']\n",
        "FETCH_INTERVAL = 300  # Fetch weather data every 5 minutes\n",
        "TEMP_THRESHOLD = 35  # Celsius - Alert threshold for temperature\n",
        "ALERT_CONSECUTIVE_READINGS = 2  # Trigger alert if temperature exceeds threshold twice\n",
        "\n",
        "# Storage for daily weather data\n",
        "weather_data_store = defaultdict(list)\n",
        "\n",
        "local_tz = pytz.timezone('Asia/Kolkata')\n",
        "\n",
        "# Function to convert UTC to local time\n",
        "def utc_to_local(utc_dt):\n",
        "    local_dt = utc_dt.replace(tzinfo=pytz.utc).astimezone(local_tz)\n",
        "    return local_tz.normalize(local_dt)\n",
        "\n",
        "\n",
        "# Function to fetch weather data for a city\n",
        "def fetch_weather(city):\n",
        "    url = f\"http://api.openweathermap.org/data/2.5/weather?q={city}&appid={API_KEY}\"\n",
        "    response = requests.get(url)\n",
        "    data = response.json()\n",
        "\n",
        "    if response.status_code == 200:\n",
        "        # Convert Unix timestamp (in UTC) to local time\n",
        "        utc_time = pd.to_datetime(data['dt'], unit='s')\n",
        "        local_time = utc_to_local(utc_time)\n",
        "\n",
        "        weather = {\n",
        "            'city': city,\n",
        "            'temp': data['main']['temp'] - 273.15,  # Kelvin to Celsius\n",
        "            'feels_like': data['main']['feels_like'] - 273.15,\n",
        "            'main_condition': data['weather'][0]['main'],\n",
        "            'timestamp': local_time  # Store the local time\n",
        "        }\n",
        "        print(f\"Data fetched for {city} at {local_time.strftime('%Y-%m-%d %H:%M:%S')}\")  # Print local time\n",
        "        return weather\n",
        "    else:\n",
        "        print(f\"Error fetching data for {city}: {data}\")\n",
        "        return None\n",
        "\n",
        "# ... (Rest of the code remains the same) ...\n",
        "\n",
        "# Function to aggregate daily weather data\n",
        "def aggregate_daily_data(storage):\n",
        "    df = pd.DataFrame(storage)\n",
        "    df['date'] = df['timestamp'].dt.date\n",
        "    daily_summary = df.groupby('date').agg(\n",
        "        avg_temp=('temp', 'mean'),\n",
        "        max_temp=('temp', 'max'),\n",
        "        min_temp=('temp', 'min'),\n",
        "        dominant_condition=('main_condition', lambda x: x.mode()[0])\n",
        "    )\n",
        "    return daily_summary\n",
        "\n",
        "# Function to check for temperature threshold breaches\n",
        "def check_threshold(storage, city):\n",
        "    if len(storage[city]) >= ALERT_CONSECUTIVE_READINGS:\n",
        "        recent_data = storage[city][-ALERT_CONSECUTIVE_READINGS:]\n",
        "        if all(item['temp'] > TEMP_THRESHOLD for item in recent_data):\n",
        "            print(f\"ALERT: Temperature in {city} exceeded {TEMP_THRESHOLD}°C for {ALERT_CONSECUTIVE_READINGS} consecutive readings\")\n",
        "\n",
        "# Function to plot daily weather summaries\n",
        "def plot_daily_summary(daily_summary):\n",
        "    daily_summary.plot(y=['avg_temp', 'max_temp', 'min_temp'], kind='line', marker='o', title=\"Daily Weather Summary\")\n",
        "    plt.ylabel('Temperature (°C)')\n",
        "    plt.show()\n",
        "\n",
        "# Main function to fetch and process weather data\n",
        "def main():\n",
        "    while True:\n",
        "        for city in CITIES:\n",
        "            weather_data = fetch_weather(city)\n",
        "            if weather_data:\n",
        "                weather_data_store[city].append(weather_data)\n",
        "                print(f\"Fetched data: {weather_data}\")\n",
        "\n",
        "                # Process daily data and generate rollups\n",
        "                daily_summary = aggregate_daily_data(weather_data_store[city])\n",
        "\n",
        "                # Check if temperature exceeds the threshold\n",
        "                check_threshold(weather_data_store, city)\n",
        "\n",
        "                # Visualize daily summary at the end of the day\n",
        "                if datetime.now().hour == 23 and datetime.now().minute >= 55:\n",
        "                    plot_daily_summary(daily_summary)\n",
        "\n",
        "        # Sleep for the defined interval before fetching the next set of data\n",
        "        time.sleep(FETCH_INTERVAL)\n",
        "\n",
        "# Run the weather monitoring system\n",
        "if __name__ == '__main__':\n",
        "    main()"
      ],
